using System;

namespace _5._6
{
    class Program
    {
        static void Main(string[] args)
        {
            string speach = Console.ReadLine();
            string output;

            switch (speach)
            {
                case "England":
                case "USA":
                    output = "English";
                    break;

                case "Spain":
                case "Argentina":
                case "Mexico":
                    output = "Spanish";
                    break;
                default:
                    output = "unknown";
                    break;
            }
            Console.WriteLine(output);
        }
    }
}
